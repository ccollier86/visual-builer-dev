{
  "id": "tokens_reference",
  "version": "1.0.0",
  "typography": {
    "fontFamily": "'Open Sans', Arial, sans-serif",
    "baseSizePx": 14,
    "scale": 1.25,
    "lineHeight": 1.6,
    "monospaceFamily": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, monospace"
  },
  "color": {
    "text": "#000000",
    "muted": "#555555",
    "accent": "#666666",
    "border": "#cccccc",
    "background": "#ffffff"
  },
  "spacing": {
    "unitPx": 12,
    "scale": 1.25
  },
  "table": {
    "density": "normal",
    "borders": "row",
    "striped": false
  },
  "list": {
    "bulletStyle": "disc",
    "numberStyle": "decimal"
  },
  "layout": {
    "sectionGap": 20,
    "paragraphGap": 12,
    "headerShowTitle": true
  },
  "print": {
    "pageSize": "Letter",
    "margin": "0.75in",
    "showHeader": true,
    "showFooter": true,
    "monochrome": false
  },
  "brand": {
    "logoUrl": "",
    "headerHtml": "",
    "footerHtml": ""
  }
}
