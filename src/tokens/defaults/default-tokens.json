{
  "id": "tokens_reference",
  "version": "1.0.0",
  "typography": {
    "fontFamily": "'Open Sans', Arial, sans-serif",
    "baseSizePx": 14,
    "scale": 1.25,
    "lineHeight": 1.6,
    "monospaceFamily": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, monospace"
  },
  "color": {
    "text": "#000000",
    "muted": "#666666",
    "accent": "#666666",
    "border": "#cccccc",
    "background": "#ffffff"
  },
  "spacing": {
    "unitPx": 12,
    "scale": 1.25
  },
  "table": {
    "density": "normal",
    "borders": "row",
    "striped": false
  },
  "list": {
    "bulletStyle": "disc",
    "numberStyle": "decimal"
  },
  "layout": {
    "sectionGap": 20,
    "paragraphGap": 12,
    "headerShowTitle": true,
    "pagePaddingPx": 40,
    "containerPaddingPx": 60,
    "containerMaxWidthPx": 1000,
    "headerGapPx": 20,
    "headerPaddingY": 20,
    "sectionBanner": {
      "marginPx": 60,
      "paddingY": 8,
      "paddingLeftPx": 75,
      "borderWidthPx": 4
    }
  },
  "print": {
    "pageSize": "Letter",
    "margin": "0.75in",
    "showHeader": true,
    "showFooter": true,
    "monochrome": false
  },
  "brand": {
    "logoUrl": "",
    "headerHtml": "",
    "footerHtml": ""
  },
  "surface": {
    "headerBackground": "#ffffff",
    "headerBorder": "#cccccc",
    "sectionBannerBackground": "#e8e8e8",
    "sectionBannerBorder": "#999999",
    "cardBackground": "#ffffff",
    "cardBorder": "#e0e0e0",
    "alertInfoBackground": "#f5f5f5",
    "alertInfoBorder": "#666666",
    "signatureBorder": "#cccccc"
  }
}
