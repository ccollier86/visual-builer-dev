{
  "id": "tokens_default",
  "version": "1.0.0",
  "typography": {
    "fontFamily": "Inter, ui-sans-serif, system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, Apple Color Emoji, Segoe UI Emoji",
    "baseSizePx": 12,
    "scale": 1.2,
    "lineHeight": 1.45,
    "monospaceFamily": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, monospace"
  },
  "color": {
    "text": "#111111",
    "muted": "#6b7280",
    "accent": "#2563eb",
    "border": "#e5e7eb",
    "background": "#ffffff"
  },
  "spacing": {
    "unitPx": 8,
    "scale": 1.25
  },
  "table": {
    "density": "compact",
    "borders": "row",
    "striped": false
  },
  "list": {
    "bulletStyle": "disc",
    "numberStyle": "decimal"
  },
  "layout": {
    "sectionGap": 16,
    "paragraphGap": 8,
    "headerShowTitle": true
  },
  "print": {
    "pageSize": "Letter",
    "margin": "0.75in",
    "showHeader": true,
    "showFooter": true,
    "monochrome": false
  },
  "brand": {
    "logoUrl": "",
    "headerHtml": "",
    "footerHtml": ""
  }
}
